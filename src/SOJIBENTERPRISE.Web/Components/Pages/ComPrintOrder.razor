@page "/PrintOrder/{OrderId:int}/{CustomerId:int}"

@rendermode @(new InteractiveServerRenderMode(prerender: false))

@if (OrderId > 0)
{
    @* <OrderDetails OrderId="@OrderId"></OrderDetails> *@
    <ComPersonHeaderInfo Id="@CustomerId"></ComPersonHeaderInfo>
    <SOJIBENTERPRISE.Web.Components.Pages.Sales.OrdersSells.ComOrderDetails OrderId="@OrderId"></SOJIBENTERPRISE.Web.Components.Pages.Sales.OrdersSells.ComOrderDetails>
}

@code {
    [Parameter]
    public int OrderId { get; set; }

    [Parameter]
    public int CustomerId { get; set; }

    protected override void OnInitialized()
    {
        base.OnInitialized();
    }
}